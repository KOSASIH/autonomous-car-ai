import pandas as pd
import matplotlib.pyplot as plt

# Load data from CSV file
data = pd.read_csv('data/data.csv')

# Explore the data
print(data.head())
print(data.describe())

# Visualize the data
plt.hist(data['steering_angle'], bins=20)
plt.xlabel('Steering Angle')
plt.ylabel('Frequency')
plt.show()
